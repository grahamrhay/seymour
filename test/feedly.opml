        </outline>
        <outline text="category2" title="category2">
            <outline type="rss" text="feed1" title="feed1" xmlUrl="http://feed1.com/rss" htmlUrl="http://feed1.com/home"/>
        </outline>
        <outline type="rss" text="feed3" title="feed3" xmlUrl="http://feed3.com/rss" htmlUrl="http://feed3.com/home"/>
